public class Treasure extends Item {
    private int value;

//skapar skattens v채rde

    public Treasure(String name, int value) {
        super(name);
        this.value = value;
    }

    @Override
    public void use() {
        System.out.println("Skatt anv채nds och 채r v채rd " + value + " guld.");
    }

    public int getValue() {
        return value;
    }
}
